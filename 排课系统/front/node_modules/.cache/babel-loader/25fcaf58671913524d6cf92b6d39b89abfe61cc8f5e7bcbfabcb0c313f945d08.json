{"ast":null,"code":"import { addStudent } from \"./api.js\";\nimport dict from \"@/views/template/dict\";\nexport default {\n  name: \"add\",\n  components: {\n    dict\n  },\n  data() {\n    return {\n      submitLoading: false,\n      // 表单提交状态\n      form: {\n        // 添加或编辑表单对象初始化数据\n        name: \"\",\n        sex: \"\",\n        age: 0,\n        number: \"\",\n        school: \"\"\n      },\n      // 表单验证规则\n      formValidate: {}\n    };\n  },\n  methods: {\n    init() {},\n    handleReset() {\n      this.$refs.form.resetFields();\n    },\n    handleSubmit() {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          addStudent(this.form).then(res => {\n            this.submitLoading = false;\n            if (res.success) {\n              this.$Message.success(\"操作成功\");\n              this.submited();\n            }\n          });\n        }\n      });\n    },\n    close() {\n      this.$emit(\"close\", true);\n    },\n    submited() {\n      this.$emit(\"submited\", true);\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};","map":{"version":3,"names":["addStudent","dict","name","components","data","submitLoading","form","sex","age","number","school","formValidate","methods","init","handleReset","$refs","resetFields","handleSubmit","validate","valid","then","res","success","$Message","submited","close","$emit","mounted"],"sources":["src/views/demo/demo1/add.vue"],"sourcesContent":["<template>\n<div>\n    <Card>\n        <div slot=\"title\">\n            <div class=\"edit-head\">\n                <a @click=\"close\" class=\"back-title\">\n                    <Icon type=\"ios-arrow-back\" />返回\n                </a>\n                <div class=\"head-name\">添加</div>\n                <span></span>\n                <a @click=\"close\" class=\"window-close\">\n                    <Icon type=\"ios-close\" size=\"31\" class=\"ivu-icon-ios-close\" />\n                </a>\n            </div>\n        </div>\n        <Form ref=\"form\" :model=\"form\" :label-width=\"100\" :rules=\"formValidate\" label-position=\"left\">\n            <FormItem label=\"姓名\" prop=\"name\">\n                <Input v-model=\"form.name\" clearable style=\"width:570px\" />\n            </FormItem>\n            <FormItem label=\"性别\" prop=\"sex\">\n                <dict dict=\"sex\" v-model=\"form.sex\" transfer clearable placeholder=\"请选择性别...\" style=\"width: 570px\" />\n            </FormItem>\n            <FormItem label=\"年龄\" prop=\"age\">\n                <InputNumber v-model=\"form.age\" min=\"0\" max=\"5000000\" style=\"width:570px\"></InputNumber>\n            </FormItem>\n            <FormItem label=\"学号\" prop=\"number\">\n                <Input v-model=\"form.number\" clearable style=\"width:570px\" />\n            </FormItem>\n            <FormItem label=\"学校\" prop=\"school\">\n                <Input v-model=\"form.school\" clearable style=\"width:570px\" />\n            </FormItem>\n            <Form-item class=\"br\">\n                <Button @click=\"handleSubmit\" :loading=\"submitLoading\" type=\"primary\">提交并保存</Button>\n                <Button @click=\"handleReset\">重置</Button>\n                <Button type=\"dashed\" @click=\"close\">关闭</Button>\n            </Form-item>\n        </Form>\n    </Card>\n</div>\n</template>\n\n<script>\nimport {\n    addStudent\n} from \"./api.js\";\nimport dict from \"@/views/template/dict\";\nexport default {\n    name: \"add\",\n    components: {\n        dict\n    },\n    data() {\n        return {\n            submitLoading: false, // 表单提交状态\n            form: { // 添加或编辑表单对象初始化数据\n                name: \"\",\n                sex: \"\",\n                age: 0,\n                number: \"\",\n                school: \"\",\n            },\n            // 表单验证规则\n            formValidate: {}\n        };\n    },\n    methods: {\n        init() {},\n        handleReset() {\n            this.$refs.form.resetFields();\n        },\n        handleSubmit() {\n            this.$refs.form.validate(valid => {\n                if (valid) {\n                    addStudent(this.form).then(res => {\n                        this.submitLoading = false;\n                        if (res.success) {\n                            this.$Message.success(\"操作成功\");\n                            this.submited();\n                        }\n                    });\n                }\n            });\n        },\n        close() {\n            this.$emit(\"close\", true);\n        },\n        submited() {\n            this.$emit(\"submited\", true);\n        }\n    },\n    mounted() {\n        this.init();\n    }\n};\n</script>\n\n<style lang=\"less\">\n// 建议引入通用样式 具体路径自行修改 可删除下面样式代码\n// @import \"../../../styles/single-common.less\";\n.edit-head {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    position: relative;\n\n    .back-title {\n        color: #515a6e;\n        display: flex;\n        align-items: center;\n    }\n\n    .head-name {\n        display: inline-block;\n        height: 20px;\n        line-height: 20px;\n        font-size: 16px;\n        color: #17233d;\n        font-weight: 500;\n        overflow: hidden;\n        text-overflow: ellipsis;\n        white-space: nowrap;\n    }\n\n    .window-close {\n        z-index: 1;\n        font-size: 12px;\n        position: absolute;\n        right: 0px;\n        top: -5px;\n        overflow: hidden;\n        cursor: pointer;\n\n        .ivu-icon-ios-close {\n            color: #999;\n            transition: color .2s ease;\n        }\n    }\n\n    .window-close .ivu-icon-ios-close:hover {\n        color: #444;\n    }\n}\n</style>\n"],"mappings":"AA0CA,SACAA,UAAA,QACA;AACA,OAAAC,IAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;IACA;MACAC,aAAA;MAAA;MACAC,IAAA;QAAA;QACAJ,IAAA;QACAK,GAAA;QACAC,GAAA;QACAC,MAAA;QACAC,MAAA;MACA;MACA;MACAC,YAAA;IACA;EACA;EACAC,OAAA;IACAC,KAAA;IACAC,YAAA;MACA,KAAAC,KAAA,CAAAT,IAAA,CAAAU,WAAA;IACA;IACAC,aAAA;MACA,KAAAF,KAAA,CAAAT,IAAA,CAA